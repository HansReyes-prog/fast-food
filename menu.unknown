import React, { useEffect, useState } from 'react'; 
function Menu() {
 const [menu, setMenu] = useState([]); 

useEffect(() => { 
fetch('https://api.mockable.io/menu')
.then(response => response.json()) 
.then(data => setMenu(data.menu)); 
}, []); 

return ( 
<div> 
<h1>Men√∫</h1> 
<ul> 
{menu.map(item => ( 
<li key={item.id}>
 <h2>{item.name}</h2> 
<p>{item.description}</p>
 <p>${item.price}</p>
 </li> 
))}
 </ul> 
</div> 
); 
} 
export default Menu;
